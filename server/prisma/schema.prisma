generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Question{
    id Int @id @default(autoincrement())
    question String
    option1 String
    option2 String
    option3 String
    option4 String
    answer String
    paperId String
    QuestionPAper QuestionPaper @relation(fields: [paperId],references: [id])
}

model QuestionPaper{
    id String @id @default(uuid())
    title String
    questions Question[]
}
