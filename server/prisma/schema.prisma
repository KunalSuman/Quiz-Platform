generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model question{
    id Int @id @default(autoincrement())
    question String
    option1 String
    option2 String
    option3 String
    option4 String
    answer String
    paperid String  @map("paperid")
    questionpaper questionpaper @relation(fields: [paperid],references: [id])
}

model questionpaper{
    id String @id @default(uuid())
    title String
    questions question[]
    @@map("questionpaper")
}
